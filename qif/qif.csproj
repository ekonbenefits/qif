<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company>https://github.com/shaewalters/qif</Company>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyName>QifApi</AssemblyName>
    <AssemblyTitle>QifApi</AssemblyTitle>
    <Product>QifApi</Product>
    <PackageId>QifApi</PackageId>
    <Description>Quicken Interchange Format API (.NET Managed Wrapper)</Description>
    <Copyright>Copyright © 2017</Copyright>
    <Version>1.1.1</Version>
    <Authors>Shane Walters</Authors>
    <PackageProjectUrl>https://github.com/shanewalters/qif</PackageProjectUrl>
    <PackageTags>dotnetcore netcore nuget dotnetstandard netstandard qif qifapi quicken</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
</Project>